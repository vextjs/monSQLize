{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "monsqlize": ["./index.d.ts"]
    }
  },
  "include": [
    "test/types/**/*.ts",
    "index.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}

